{
  "name": "zod-prisma2",
  "version": "0.0.1",
  "description": "A Prisma generator that creates Zod schemas for all of your models",
  "license": "MIT",
  "author": "AdÃ£o Santos",
  "homepage": "https://github.com/adaosantos/zod-prisma2#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/adaosantos/zod-prisma2.git"
  },
  "bugs": {
    "url": "https://github.com/adaosantos/zod-prisma2/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "keywords": ["zod", "prisma", "generator"],
  "files": ["dist"],
  "scripts": {
    "build": "NODE_ENV=production tsup",
    "lint": "biome check",
    "lint:fix": "biome check --write"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.15.4",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.12.0",
    "esbuild": "^0.24.2",
    "lefthook": "^1.10.10",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "tsup": {
    "entry": ["src/index.ts"],
    "splitting": true,
    "skipNodeModulesBundle": true,
    "sourcemap": false,
    "minify": true,
    "format": ["cjs", "esm"],
    "clean": true,
    "dts": true,
    "outDir": "dist",
    "treeshake": true,
    "platform": "neutral",
    "target": "es2018"
  }
}
